{
  "name": "bpmn-analyzer-pro",
  "version": "3.0.0",
  "description": "Professional BPMN Analysis Tool with ISO 9001 Compliance",
  "main": "src/main.js",
  "scripts": {
    "start": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --watch --mode development",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\"",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "build:stats": "webpack --mode production --profile --json > stats.json",
    "deploy:gh-pages": "npm run build:prod && gh-pages -d dist",
    "bundle:analyze": "webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "bpmn",
    "process-mining",
    "iso9001",
    "business-process",
    "analysis",
    "optimization"
  ],
  "author": "Jonas ZeuÃŸel",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "html-webpack-plugin": "^5.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "jest": "^29.6.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "gh-pages": "^5.0.0"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "jszip": "^3.10.1",
    "file-saver": "^2.0.5",
    "pdfmake": "^0.2.7",
    "xlsx": "^0.18.5",
    "bpmn-moddle": "^8.0.0",
    "dagre": "^0.8.5",
    "d3": "^7.8.0",
    "lodash": "^4.17.21"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ]
}
